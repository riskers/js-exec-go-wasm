{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Node16",
    "strict": true,
    "declaration": false,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}